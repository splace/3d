Transform {
  translation 0 1.4 0.8
  children [
    Shape {
      appearance Appearance {
        material DEF red Material {
          diffuseColor 1 1 1
          specularColor 1 1 1
          emissiveColor 1 0 0
          shininess 1
        }
      }
      geometry Sphere {
        radius 0.15
      }
    }
    DEF lt1 Script {
      eventIn        SFTime  ti
      field          SFNode  l USE red
      field          SFColor c1 0 0 0
      field          SFColor c2 1 0 0

      url "javascript:
			function initialize() {
				c2=l.emissiveColor
			}
			function ti(t) {
				if (l.emissiveColor.r != c1.r || l.emissiveColor.g != c1.g || l.emissiveColor.b != c1.b) {
					l.emissiveColor=c1
				} else{
					l.emissiveColor=c2
				}
				
				}
				"
      directOutput TRUE
    }
    DEF t1 TimeSensor {
    	loop TRUE 
		ROUTE t1.cycleTime TO lt1.ti
    }
  ]
}

